# Sheba Admin Frontend Environment Variables
# Copy this file to .env and update values as needed

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000/api

# Development API URL (if different from production)
VITE_API_BASE_URL_DEV=http://localhost:8000/api

# Application Configuration
VITE_APP_NAME=Sheba Admin Dashboard
VITE_APP_VERSION=1.0.0

# Authentication
VITE_TOKEN_KEY=auth_token
VITE_USER_KEY=user_data

# API Timeouts (in milliseconds)
VITE_API_TIMEOUT=30000

# Development/Production flags
VITE_IS_DEVELOPMENT=true
